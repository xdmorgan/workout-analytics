{
  "name": "peloton-workouts",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@nivo/bar": "^0.65.1",
    "@nivo/calendar": "^0.65.1",
    "@nivo/core": "^0.65.0",
    "@nivo/line": "^0.65.1",
    "@nivo/pie": "^0.65.1",
    "@nivo/scatterplot": "^0.65.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.10",
    "@types/gtag.js": "^0.0.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "classnames": "^2.2.6",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.16.1",
    "node-sass": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "skeletor": "skeletor --output public/skeletor --no-sourcemap",
    "typebeast": "typebeast --output public/typebeast --no-sourcemap",
    "serve": "serve -s build -p 3000",
    "format:check": "prettier --check \"{src,docs}/**/*.{md,js,ts,tsx,jsx}\"",
    "format:fix": "prettier --write \"{src,docs}/**/*.{md,js,ts,tsx,jsx}\"",
    "generate": "npx hygen",
    "postgenerate": "yarn format:fix && yarn test -u --watchAll=false",
    "generate:transformer": "yarn generate transformer new",
    "generate:page-app": "yarn generate page app",
    "generate:page-other": "yarn generate page other"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@skeletor/css": "^0.17.1",
    "serve": "^11.3.2",
    "typebeast": "^0.7.7"
  },
  "jest": {
    "globalSetup": "<rootDir>/jest.setup.js",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
